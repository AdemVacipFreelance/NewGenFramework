@model BayiPuan.MvcWebUi.Models.ComplexTypeModels.ViewModel
<div class="w3-card w3-white w3-padding">
    <div class="w3-content w3-display-container">
        @foreach (var image in Model.CampaignView)
        {
            var base64 = Convert.ToBase64String(image.CampaignImage);
            var imgSrc = String.Format("data:image/gif;base64,{0}", base64);
            <img class="mySlides" src="@imgSrc" style="width: 100%; height: 350px;">
        }

        <div class="w3-center w3-container w3-section w3-large w3-text-white w3-display-bottommiddle" style="width: 100%">
            <div class="w3-left w3-hover-text-khaki" onclick="plusDivs(-1)">&#10094;</div>
            <div class="w3-right w3-hover-text-khaki" onclick="plusDivs(1)">&#10095;</div>
            @foreach (var image in Model.CampaignView)
            {
                <span class="w3-badge demo w3-border w3-transparent w3-hover-white" onclick="currentDiv(@image.CampaignId)"></span>
            }
        </div>
    </div>
</div>